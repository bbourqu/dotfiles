# Set default values for all following accounts:
auth           on
tls            on
tls_starttls   on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile        ~/.msmtp.log

# Gmail 1
host           smtp.gmail.com
port           587
from           chase.hallsted@gmail.com
user           chase.hallsted
passwordeval   "gpg -d ~/git/dotfiles/mutt/passwords.gpg | awk '/GMail:/ {print $2}'"
